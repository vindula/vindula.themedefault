<?xml version="1.0"?>
<object>
	
	<order manager="plone.portalheader" skinname="*">
 		<viewlet name="vindula.themedefault.favicon" insert-before="plone.skip_links"/>
		<viewlet name="vindula.themedefault.usefullinks" insert-before="plone.skip_links"/>
		<viewlet name="vindula.themedefault.logotop" insert-before="plone.global_sections"/>
	</order>
	
	
	<order manager="plone.abovecontent" skinname="*">
 		<viewlet name="vindula.themedefault.menu"   insert-before="plone.path_bar"/>
		<viewlet name="vindula.themedefault.banner" insert-after="plone.path_bar"/>
	</order>
	
	
	<order manager="plone.portalfooter" skinname="*">
 		<viewlet name="vindula.themedefault.footer" insert-before="plone.footer"/>
	</order>
	
</object> 